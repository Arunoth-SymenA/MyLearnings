43. Asyncio example

import asyncio

async def task(name, duration):
    print(f"{name} started")
    await asyncio.sleep(duration)
    print(f"{name} finished")

async def main():
    await asyncio.gather(task("Task1", 2), task("Task2", 1), task("Task3", 3))

asyncio.run(main())

44. Enum usage

from enum import Enum

class Color(Enum):
    RED = 1
    GREEN = 2
    BLUE = 3

